<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="AVF.CourseParticipation.Views.ParticipantsSelectionPage" Style="{StaticResource ContentPageStyle}" Title="Teilnehmer auswählen">
    <ContentPage Title="Zuletzt anwesend">
        <StackLayout>
            <Button Text="Speichern und zurück" Command="{Binding SaveCommand}"></Button>
        </StackLayout>
    </ContentPage>
</TabbedPage>