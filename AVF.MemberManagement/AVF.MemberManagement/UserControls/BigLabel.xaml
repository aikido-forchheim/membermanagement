<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AVF.MemberManagement.UserControls.BigLabel"
             x:Name="This"
             >
  <ContentView.Content>
        <Label FontSize="{Binding FontSize, Source={x:Reference This}}" Text="{Binding Text, Source={x:Reference This}}" FontAttributes="{Binding FontAttributes, Source={x:Reference This}}" x:Name="Label">
        </Label>
  </ContentView.Content>
</ContentView>